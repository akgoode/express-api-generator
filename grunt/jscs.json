{
  "options": {
    "config": ".jscsrc"
  },

  "status": {
    "src": [
      "<%= paths.src.all %>",
      "<%= paths.grunt.all %>"
    ],
    "options": {
      "force": true
    }
  },

  "write": {
    "src": [
      "<%= paths.src.all %>",
      "<%= paths.grunt.all %>"
    ],
    "options": {
      "fix": true
    }
  }
}
